LinkedListNode<int>* removeKthNode(LinkedListNode<int> *head, int K)
{
    // Write your code here.
    if(head==NULL || K==0)
        return head;
    
    LinkedListNode<int> *slow=head,*fast=head;
    while(K--)
        fast=fast->next;
    if(fast==NULL)
        return slow->next;
    
    while(fast->next )
    {
        slow=slow->next;
        fast=fast->next;
    }
  
    slow->next=slow->next->next;
   
    return head;
}
